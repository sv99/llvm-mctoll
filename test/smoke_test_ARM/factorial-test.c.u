// CHECK: Factorial of 10 3628800

#include <stdio.h>

// extern int factorial(int n);
int factorial(int n) {
  if (n == 0) {
    return 1;
  }
  return n * factorial(n - 1);
}

int main() {
  printf("Factorial of 10 %d\n", factorial(10));
  return 0;
}
